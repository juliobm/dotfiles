set ssl_starttls=yes
set ssl_force_tls=yes
set imap_authenticators="plain"
set from = "vip@z.com"
set realname = "vip"
set imap_user = "vip@z.com"
#set imap_pass = "contra"
set folder = "imap://mail.z.com"
set spoolfile = "+INBOX"
set postponed ="+Drafts"
set header_cache =~/.mutt/cache/headers
set message_cachedir =~/.mutt/cache/bodies
set certificate_file =~/.mutt/certificates
#set smtp_pass = "contra"
set smtp_url = "smtp://vip@z.com@smtp.z.com:25/"
set move = no 
set imap_keepalive = 900
set editor = vim
# Where to save copies of outgoing mail
set record = '+Sent'
set send_charset="iso-8859-15:utf-8"
# How often to check for new mail (time in seconds).
set mail_check = 120
# obtener correo IMAP
bind index G imap-fetch-mail
